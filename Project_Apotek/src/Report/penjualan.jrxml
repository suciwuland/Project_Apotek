	<queryString language="SQL">
		<![CDATA[SELECT
     tb_transaksi.`kd_transaksi` AS tb_transaksi_kd_transaksi,
     tb_transaksi.`Tanggal` AS tb_transaksi_Tanggal,
     tb_transaksi.`user` AS tb_transaksi_user,
     tb_transaksi.`obat` AS tb_transaksi_obat,
     tb_transaksi.`harga` AS tb_transaksi_harga,
     tb_transaksi.`jumlah` AS tb_transaksi_jumlah,
     tb_transaksi.`total_harga` AS tb_transaksi_total_harga,
     tb_user.`id_user` AS tb_user_id_user,
     tb_user.`Username` AS tb_user_Username,
     tb_user.`Password` AS tb_user_Password,
     tb_user.`Level` AS tb_user_Level,
     tb_barang.`kd_obat` AS tb_barang_kd_obat,
     tb_barang.`nama_obat` AS tb_barang_nama_obat,
     tb_barang.`jenis` AS tb_barang_jenis,
     tb_barang.`satuan` AS tb_barang_satuan,
     tb_barang.`stok` AS tb_barang_stok,
     tb_barang.`harga_beli` AS tb_barang_harga_beli,
     tb_barang.`harga_jual` AS tb_barang_harga_jual
FROM
     `tb_user` tb_user INNER JOIN `tb_transaksi` tb_transaksi ON tb_user.`id_user` = tb_transaksi.`user`
     INNER JOIN `tb_barang` tb_barang ON tb_transaksi.`obat` = tb_barang.`kd_obat`]]>
