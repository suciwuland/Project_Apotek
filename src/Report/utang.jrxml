     tb_user.`id_user` AS tb_user_id_user,
     tb_user.`Username` AS tb_user_Username,
     tb_user.`Password` AS tb_user_Password,
     tb_user.`Level` AS tb_user_Level
FROM
     `tb_akun` tb_akun INNER JOIN `tb_bukukas` tb_bukukas ON tb_akun.`kd_akun` = tb_bukukas.`Kategori`
     INNER JOIN `tb_user` tb_user ON tb_bukukas.`User` = tb_user.`id_user`WHERE tb_akun.kd_akun=103 OR tb_akun.kd_akun=202]]>
