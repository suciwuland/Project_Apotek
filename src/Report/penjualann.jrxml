	<queryString language="SQL">
		<![CDATA[SELECT
     tb_transaksi.`kd_transaksi` AS tb_transaksi_kd_transaksi,
     tb_transaksi.`Tanggal` AS tb_transaksi_Tanggal,
     tb_transaksi.`user` AS tb_transaksi_user,
     tb_transaksi.`obat` AS tb_transaksi_obat,
     tb_transaksi.`harga` AS tb_transaksi_harga,
     tb_transaksi.`jumlah` AS tb_transaksi_jumlah,
     tb_transaksi.`total_harga` AS tb_transaksi_total_harga,
     tb_barang.`kd_obat` AS tb_barang_kd_obat,
     tb_barang.`nama_obat` AS tb_barang_nama_obat,
     tb_barang.`jenis` AS tb_barang_jenis,
     tb_barang.`satuan` AS tb_barang_satuan,
     tb_barang.`stok` AS tb_barang_stok,
     tb_barang.`harga_beli` AS tb_barang_harga_beli,
     tb_barang.`harga_jual` AS tb_barang_harga_jual,
     tb_user.`id_user` AS tb_user_id_user,
     tb_user.`Username` AS tb_user_Username,
     tb_user.`Password` AS tb_user_Password,
     tb_user.`Level` AS tb_user_Level,
     tb_karyawan.`id_karyawan` AS tb_karyawan_id_karyawan,
     tb_karyawan.`user` AS tb_karyawan_user,
     tb_karyawan.`nama_karyawan` AS tb_karyawan_nama_karyawan,
     tb_karyawan.`JK` AS tb_karyawan_JK,
     tb_karyawan.`email` AS tb_karyawan_email,
     tb_karyawan.`status` AS tb_karyawan_status,
     tb_karyawan.`no_tlp` AS tb_karyawan_no_tlp,
     tb_karyawan.`kewarganegaraan` AS tb_karyawan_kewarganegaraan,
     tb_karyawan.`pendidikan` AS tb_karyawan_pendidikan,
     tb_karyawan.`alamat` AS tb_karyawan_alamat,
     tb_karyawan.`gaji_pokok` AS tb_karyawan_gaji_pokok,
     tb_satuan.`id_satuan` AS tb_satuan_id_satuan,
     tb_satuan.`satuan` AS tb_satuan_satuan,
     tb_jenis.`jenis` AS tb_jenis_jenis
FROM
     `tb_barang` tb_barang INNER JOIN `tb_transaksi` tb_transaksi ON tb_barang.`kd_obat` = tb_transaksi.`obat`
     INNER JOIN `tb_user` tb_user ON tb_transaksi.`user` = tb_user.`id_user`
     INNER JOIN `tb_karyawan` tb_karyawan ON tb_user.`id_user` = tb_karyawan.`user`
     INNER JOIN `tb_satuan` tb_satuan ON tb_barang.`satuan` = tb_satuan.`id_satuan`
     INNER JOIN `tb_jenis` tb_jenis ON tb_barang.`jenis` = tb_jenis.`id_jenis`]]>
	</queryString>
